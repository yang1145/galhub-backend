# 服务器配置
PORT=3000
NODE_ENV=development

# PostgreSQL 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_NAME=galhub

# JWT 配置
# 生产环境必须设置一个强密钥（建议至少32个字符）
JWT_SECRET=your_jwt_secret_key_here

# CORS 配置
# 多个来源用逗号分隔，例如：http://localhost:3000,https://example.com
CORS_ORIGIN=http://localhost:3000
